          <techName>Reference Frame Datum Points</techName>
          <infoName>Inspection</infoName>
        </dmTitle>
      </dmAddressItems>
    </dmAddress>
    <dmStatus issueType="new">
      <security securityClassification="01"/>
      <responsiblePartnerCompany enterpriseCode="AMP360">
        <enterpriseName>AMPEL360 Engineering</enterpriseName>
      </responsiblePartnerCompany>
      <originator enterpriseCode="AMP360"/>
      <applic>
        <displayText>
          <simplePara>All aircraft</simplePara>
        </displayText>
      </applic>
      <brexDmRef>
        <dmRef>
          <dmRefIdent>
            <dmCode modelIdentCode="AMP360" systemDiffCode="AAA" systemCode="00" 
                    subSystemCode="0" subSubSystemCode="0" assyCode="00" 
                    disassyCode="00" disassyCodeVariant="A" infoCode="00A" 
                    infoCodeVariant="A" itemLocationCode="A"/>
          </dmRefIdent>
        </dmRef>
      </brexDmRef>
